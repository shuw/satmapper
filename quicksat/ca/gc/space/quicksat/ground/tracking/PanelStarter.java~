package ca.gc.space.quicksat.ground.tracking;
import javax.swing.*;
//import java.awt.component;
/**
 *
 * @author  User
 * @version 
 */
public class PanelStarter extends Thread{
    JPanel panel;
    /** Creates new PanelThread */
    public PanelStarter(JFrame panel) {
        super();
        this.panel = panel;
    }
    synchronized public void run() {
        panel.show();
        panel.setVisible(true);
        
    }
}
